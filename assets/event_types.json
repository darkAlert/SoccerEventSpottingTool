   "supertypes": {
	   "ATTACK": {
	      "alias": "АТАКА",
	      "types": {

		 "START": {
		    "alias": "Начало",
		    "start_causes": {
		       "1": "Первая передача с игры (open play), которая начинает владение команды в своей трети поля (зоны 1–10)*, при котором (владении) все игроки соперника или все кроме одного игрока соперника в момент первой или второй передачи находятся от мяча в 7 и более метрах, и которое начинается минимум через 5 секунд после того, как мячом владела команда соперника",
		       "2": "Первая передача при ударе от ворот",
		       "3": "Ввод мяча в игру из аута или со штрафного удара в своей трети поля"
		    },
		    "end_causes": {
		       "1": "Остановка игры",
		       "2": "Потеря мяча без быстрого возврата мяча",
		       "3": "Переход команды в стадию развития атаки",
		       "4": "Переход владения на чужую половину поля",
		       "5": "Быстрая атака с завершением"
		    }
		 },

		 "DEVELOPMENT": {
		    "alias": "Развитие",
		    "start_causes": {
		       "1": "...идет продвижение мяча на чужую половину поля или в финальную треть поля",
		       "2": "...идет попытка продвижения мяча на чужую половину поля или в финальную треть поля"
		    },
		    "end_causes": {
		       "1": "Остановка игры",
		       "2": "Потеря мяча с переходом владения к сопернику",
		       "3": "Переход владения в стадию завершения атаки",
		       "4": "Переход владения в стадию начала атаки",
		       "5": "Удар и возможное продолжение фазы “завершение атаки” после удара",
		       "6": "Гол",
		       "7": "“Конец кросса” или передача в штрафную и возможное продолжение фазы “завершение атаки” после передачи в штрафную"
		    }
		 },

		 "END": {
		    "alias": "Завершение",
		    "start_causes": {
		       "1": "...идет продвижение мяча к штрафной, в штрафную, или удар по воротам",
		       "2": "...идет попытка продвижения мяча к штрафной, в штрафную, или удар по воротам"
		    },
		    "end_causes": {
		       "1": "Остановка игры",
		       "2": "Переход владения в среднюю треть поля без быстрого возврата мяча в финальную треть поля",
		       "3": "Потеря мяча с переходом владения к сопернику",
		       "4": "Удар и возможное продолжение фазы “завершение атаки” после удара",
		       "5": "Гол",
		       "6": "“Конец кросса” или передача в штрафную и возможное продолжение фазы “завершение атаки” после передачи в штрафную"
		    }
		 }
